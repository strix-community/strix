{
    "name": "strix-community/strix",
    "type": "project",
    "description": "Strix is a powerful platform to handle donations, updates, and forums.",
    "keywords": [
        "strix",
        "laravel",
        "php",
        "livewire",
        "alpinejs"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "packages/*"
        }
    ]
}
